#include<bits/stdc++.h>
using namespace std;
int main(){
    int i, j, n;

    cin>>n;
    for(i=1; i<=n; i++){
        for(j=i; j<=n; j++){
            cout<<j<<" ";
            
        }
        
        cout<<endl;
    }
    for(i=n-1; i>0; i--){
        for(j=i; j<=n; j++){
            cout<<j<<" ";
        }
        cout<<endl;
    }
    return 0;
}
